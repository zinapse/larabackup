{
    "name": "zinapse/larabackup",
    "type": "library",
    "description": "A simple package to let you backup your Laravel application's data.",
    "homepage": "https://github.com/zinapse/larabackup",
    "require": {
        "laravel/framework": "^9.11"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zinapse\\Larabackup\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zinapse"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Zinapse\\LaraBackup\\LaraBackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
